pipeline {
    agent any 
    stages {
        stage('Phase 1') {
            steps {
                echo 'This is the first phase of the pipeline aka Initialization'
                echo 'Riemann is great'
            }
        }
        stage('Phase 2') {
            steps {
                echo 'This is the second phase aka build up' 
                echo 'von Neumann is great'
            }
        }
        stage('Phase 3') {
            steps {
                echo 'This is the third phase of the pipeline aka verification' 
                echo 'Nash is great'
            }
        }
        stage('Phase 4') {
            steps {
                echo 'This is the fourth phase of the pipeline aka access provision'
                echo 'Galois is great'
            }
        }
        stage('Phase 5') {
            steps {
                echo 'This is the fifth phase of the pipeline aka modification'
                echo 'Gauss is great'
            }
        }
        stage('Phase 6') {
            steps {
                echo 'This is the sixth and last phase of the pipeline aka committing the changes'
                echo 'Leibniz is great'
                echo 'Pipeline has executed successfully'
                echo 'End of execution'
            }
        }
    }
}
